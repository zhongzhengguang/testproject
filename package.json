{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:test": "export NODE_ENV=\"test\" && next dev",
    "build": "next build",
    "build:test": "export NODE_ENV=\"test\" && next build",
    "start": "next start",
    "start:test": "export NODE_ENV=\"test\" && next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cypress:dev": "start-server-and-test 'npm run dev:test' 3000 'npx cypress open --config watchForFileChanges=false'",
    "cypress:production": "npm run build:test && start-server-and-test 'npm run start:test' 3000 'npx cypress open --config watchForFileChanges=false'"
  },
  "dependencies": {
    "next": "latest",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@cypress/react": "^7.0.2",
    "@swc/core": "^1.2.215",
    "@swc/plugin-styled-components": "^1.5.34",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/react": "18.0.9",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.1.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "start-server-and-test": "^1.15.2",
    "typescript": "4.6.4"
  }
}
